<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="myCar" class="com.jsp.demo.Car">
	<property name="name" value="bsjnsm"></property>
	<property name="brand" value="suzuki"></property>
	<property name="color" value="color"></property>
	
	</bean>
	
	<bean id="myCar2" class="com.jsp.constructor.Car2">
	<constructor-arg index="0" value="Mahindra" ></constructor-arg>
	<constructor-arg index="1" value="ABC" ></constructor-arg>
	<constructor-arg index="2" value="2540153.00" ></constructor-arg>
	
	
	</bean>
	
</beans>